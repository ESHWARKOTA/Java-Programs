import java.util.Scanner;

public class Main{
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        System.out.println("Enter the integer A and the number of times N separated by space:");
        int A = scanner.nextInt();
        int N = scanner.nextInt();
        printIntegerNTimes(A, N);
        scanner.close();
    }
    public static void printIntegerNTimes(int A, int N) {
        for (int i = 0; i < N; i++) {
            System.out.println(A);
        }
    }
}
